{
  "name": "kino-webbsite-with-webbserver",
  "version": "1.0.0",
  "description": "Uppgiften går ut på att skapa en webbserver med templating och dynamisk rendering",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "dev-vite": "vite",
    "vite-build": "vite build",
    "build": "sass src/sections/main.scss:css/main.css",
    "preview": "vite preview",
    "sass-watch": "sass --watch src/sections/main.scss:css/main.css",
    "prepare": "husky",
    "start": "node start-server.js",
    "dev-serv": "nodemon start-server.js"
  },
  "nodemonConfig": {
    "ext": "js, mjs, cjs, json, yaml, yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anteman-swe/Kino-Webbsajt-with-webserver.git"
  },
  "keywords": [],
  "author": "SJPI25 - A4 Grupp D",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anteman-swe/Kino-Webbsajt-with-webserver/issues"
  },
  "homepage": "https://github.com/anteman-swe/Kino-Webbsajt-with-webserver#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier-plugin-ejs": "^1.0.3",
    "sass": "^1.97.2",
    "stylelint": "^17.1.1",
    "stylelint-config-standard-scss": "^17.0.0",
    "supertest": "^7.2.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "braces": "^3.0.3",
    "chokidar": "^4.0.3",
    "cross-env": "^10.1.0",
    "detect-libc": "^1.0.3",
    "dotenv": "^17.2.3",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "fill-range": "^7.1.1",
    "immutable": "^5.1.4",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "marked": "^17.0.1",
    "micromatch": "^4.0.8",
    "node": "^25.4.0",
    "node-addon-api": "^7.1.1",
    "picomatch": "^2.3.1",
    "readdirp": "^4.1.2",
    "source-map-js": "^1.2.1",
    "swagger-ui-express": "^5.0.1",
    "to-regex-range": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss, css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json, md}": [
      "prettier --write"
    ],
    "*.{yaml,yml}": [
      "yamllint -c .yamllint",
      "prettier --write"
    ]
  }
}
